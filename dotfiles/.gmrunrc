ShowDotFiles = 1
TabTimeout = 0

Terminal = terminator
TermExec = ${Terminal} -e
AlwaysInTerm = ssh telnet ftp lynx mc vi vim cat top sudo man info htop mutt

History = 500

URL_http = firefox %u
URL_info = ${TermExec} info %s
URL_man = ${TermExec} 'man %s'
URL_info = ${TermExec} 'info %s'
URL_file = exo-open %s
URL_f = exo-open %s
URL_g = firefox 'http://www.google.com/search?q=%s'
